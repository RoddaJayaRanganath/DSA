void insert_at_last(struct node **head,int deta)
{
    struct node *neww =  (struct node *)malloc(sizeof(struct node));
    neww->data = deta;
    neww->next = NULL;
    
    if(*head == NULL)
    {
        *head = neww;
    }
    else
    {
        struct node *temp = *head;
        while(temp->next != NULL)
        {
            temp = temp->next;
        }
        temp->next = neww;
    }
}
